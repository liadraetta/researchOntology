#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#authoredBy
:authoredBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain reo:Work ;
            rdfs:range foaf:Person .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#editedBy
:editedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf reo:edits ;
          rdfs:domain reo:Work .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#funds
:funds rdf:type owl:ObjectProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( foaf:Organization
                                   foaf:Person
                                 )
                   ] ;
       rdfs:range foaf:Project .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#hasAffiliaton
:hasAffiliaton rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain foaf:Person ;
               rdfs:range :Affiliation .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain foaf:Person ;
         rdfs:range reo:Role .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#isAuthorOf
:isAuthorOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain foaf:Person ;
            rdfs:range reo:Work .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#isFundedBy
:isFundedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain foaf:Project ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( foaf:Organization
                                       foaf:Person
                                     )
                       ] .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#isPartOf
:isPartOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( reo:Work
                                      foaf:Person
                                    )
                      ] ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( foaf:Organization
                                     foaf:Project
                                   )
                     ] .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#isRelatedTo
:isRelatedTo rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( reo:Work
                                         foaf:Person
                                         foaf:Project
                                       )
                         ] ;
             rdfs:range reo:Topic .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#memberOf
:memberOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain foaf:Person ;
          rdfs:range foaf:Organization .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology#takesPartIn
:takesPartIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( foaf:Organization
                                         foaf:Person
                                       )
                         ] ;
             rdfs:range foaf:Project .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology/edits
reo:edits rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:range reo:Work .


###  http://www.semanticweb.org/liadraetta/ontologies/2024/8/research-ontology/manages
reo:manages rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain foaf:Person ;
            rdfs:range foaf:Organization ,
                       foaf:Project .


